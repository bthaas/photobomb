{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai-api-key",
    "GOOGLE_API_KEY": "@google-api-key", 
    "AI_PROVIDER": "@ai-provider"
  }
}